{
  "name": "jinwoo-engineering",
  "description": "",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch:js & npm run watch:css & npm run watch:html",
    "watch:js": "npx babel src/js --watch --out-dir public/js",
    "watch:css": "npx sass --watch --style=compressed src/scss:public/css",
    "watch:html": "nodemon -e html -w ./src -x \"npm run minify:html\"",
    "minify:html": "npx html-minifier --file-ext html --input-dir ./src --output-dir ./public --collapse-whitespace --collapse-boolean-attributes --remove-comments --remove-empty-attributes  --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-style-link-type-attributes --remove-tag-whitespace --sort-attributes --sort-class-name --trim-custom-fragments --minify-css --minify-js",
    "eslint": "eslint -c .eslintrc.json src/js/*.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jinwoo-engineering/jinwoo-engineering.github.io.git"
  },
  "author": "thegicode",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jinwoo-engineering/jinwoo-engineering.github.io/issues"
  },
  "homepage": "https://jinwoo-engineering.github.io/",
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/plugin-transform-object-assign": "^7.12.13",
    "@babel/plugin-transform-template-literals": "^7.13.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.5",
    "babel-plugin-remove-template-literals-whitespace": "^1.0.3",
    "babel-plugin-transform-inline-consecutive-adds": "^0.4.3",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "babel-plugin-transform-remove-undefined": "^0.5.0",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "html-minifier": "^4.0.0",
    "nodemon": "^2.0.7",
    "sass": "^1.32.8"
  }
}
